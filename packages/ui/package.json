{
  "name": "@easy-ui/ui",
  "version": "0.0.1",
  "description": "Easy-UI 组件库",
  "type": "module",
  "main": "./dist/easy-ui.cjs.js",
  "module": "./dist/easy-ui.es.js",
  "types": "./dist/index.d.ts",
